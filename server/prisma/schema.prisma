generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum AssetCategory {
  stock
  fii
}

enum AssetType {
  ON
  PN
  UNIT
}

model Asset {
  id           String        @id @default(uuid())
  code         String        @unique
  fantasy_name String
  company_name String
  cnpj         String
  category     AssetCategory
  type         AssetType

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("assets")
}
